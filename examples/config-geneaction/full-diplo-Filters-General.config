default:
 ploidy            : 2
 genotypeFile      : "example-genotype-diplo.csv"
 phenotypeFile     : "example-phenotype.csv"
 significanceLevel : 0.05
 correctionMethod  : "Bonferroni"     
 gwasModel         : "Full"
 nBest             : 10
 filtering         : TRUE
 MAF               : 0.01   #0.001    
 MIND              : 0.1    #0.5      
 GENO              : 0.1    #0.5      
 HWE               : 1e-10  #1e-50    
 tools             : "GWASpoly SHEsis PLINK TASSEL"

advanced:
  gwaspoly_geneaction : "general"     # "ALL" or "additive" or "general" or "diplo-additive" or "diplo-general" or "1-dom" or "2-dom" (2-dom only for tetraploides)  
  tassel_geneaction   : "general"     # "ALL" or "additive" or "general" or "dominant" 
  plink_geneaction    : "genotypic"    # "additive" or "genotypic" or "dominant" or "recessive"
  shesis_geneaction   : "NONE"     # "NONE" or "additive" (Only one gene actions)


